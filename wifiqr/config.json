{
    "name": "wifiqr",
    "version": "2021.04.18.02",
    "slug": "wifiqr",
    "description": "Wifiqr add-on",
    "url": "https://github.com/fliphess/addons/tree/master/wifiqr",
    "startup": "services",
    "hassio_api": false,
    "hassio_role": "default",
    "arch": [
        "aarch64",
        "amd64",
        "armhf",
        "armv7",
        "i386"
    ],
    "options": {
      "wifi_ssid": "!secret wifissid",
      "wifi_password": "!secret wifipassword",
      "debug": false
    },
    "schema": {
      "wifi_ssid": "str",
      "wifi_password": "str",
      "debug": "bool"
    },
    "audio": false,
    "boot": "auto",
    "webui": "http://[HOST]:[PORT:80]/",
    "ingress": true,
    "ingress_port": 8000,
    "ports": {
        "8000/tcp": 8000
    },
    "ports_description": {
        "8000/tcp": "WifiQR webserver port"
    },
    "panel_icon": "mdi:qrcode",
    "panel_title": "WIFI Password",
    "image": "flapperdeflipper/addon-wifiqr"
}
